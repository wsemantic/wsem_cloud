<odoo>
    <record id="view_partner_form_inherit_cloud_url" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.cloud.url</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='website']" position="after">
                <field name="cloud_url"/>
            </xpath>
        </field>
    </record>
</odoo>
